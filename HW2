      "cell_type": "markdown",
      "source": [
        "**Question 2**  "
      ],
      "metadata": {
        "id": "x9HIubF5RvP0"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# true success probabilities for the arms\n",
        "theta_true1, theta_true2 = 0.2, 0.8\n",
        "\n",
        "# initialize Beta distribution parameters\n",
        "alpha_1, beta_1 = 1, 1\n",
        "alpha_2, beta_2 = 1, 1\n",
        "\n",
        "# number of iterations\n",
        "n = 1000\n",
        "\n",
        "# tracking results\n",
        "rewards = []\n",
        "cumulative_rewards = []\n",
        "posterior_means_arm1 = []\n",
        "posterior_means_arm2 = []\n",
        "\n",
        "for t in range(n):\n",
        "    sampled_theta1 = np.random.beta(alpha_1, beta_1)\n",
        "    sampled_theta2 = np.random.beta(alpha_2, beta_2)\n",
        "\n",
        "    if sampled_theta1 > sampled_theta2:\n",
        "        reward = np.random.binomial(1, theta_true1)\n",
        "        alpha_1 += reward\n",
        "        beta_1 += 1 - reward\n",
        "    else:\n",
        "        reward = np.random.binomial(1, theta_true2)\n",
        "        alpha_2 += reward\n",
        "        beta_2 += 1 - reward\n",
        "\n",
        "    # update tracking\n",
        "    rewards.append(reward)\n",
        "    cumulative_rewards.append(sum(rewards))\n",
        "    posterior_means_arm1.append(alpha_1 / (alpha_1 + beta_1))\n",
        "    posterior_means_arm2.append(alpha_2 / (alpha_2 + beta_2))\n",
        "\n",
        "# plots\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.plot(cumulative_rewards, label=\"Cumulative Rewards\")\n",
        "plt.title(\"Cumulative Rewards Over Time\")\n",
        "plt.xlabel(\"Iterations\")\n",
        "plt.ylabel(\"Total Rewards\")\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.plot(posterior_means_arm1, label=\"Posterior Mean Arm 1\")\n",
        "plt.plot(posterior_means_arm2, label=\"Posterior Mean Arm 2\")\n",
        "plt.axhline(theta_true1, color=\"blue\", linestyle=\"--\", label=\"True 1\")\n",
        "plt.axhline(theta_true2, color=\"orange\", linestyle=\"--\", label=\"True 2\")\n",
        "plt.title(\"Posterior Means Over Time\")\n",
        "plt.xlabel(\"Iterations\")\n",
        "plt.ylabel(\"Probability\")\n",
        "plt.legend()\n",
        "plt.show()\n"
      ],
