      "cell_type": "markdown",
      "source": [
        "**Question 3**"
      ],
      "metadata": {
        "id": "zHuGID5Rjqtv"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "theta_true = [0.2, 0.5, 0.8]  # probabilities for bandit 1, 2, and 3\n",
        "\n",
        "alpha = [1, 1, 1]\n",
        "beta = [1, 1, 1]\n",
        "\n",
        "alpha_dirichlet = np.ones(3) # initialize dirichlet prior parameters\n",
        "\n",
        "n = 1000\n",
        "\n",
        "rewards = []\n",
        "cumulative_rewards = []\n",
        "posterior_means = [[] for _ in range(3)]\n",
        "posterior_means_dirichlet = []\n",
        "\n",
        "for t in range(n):\n",
        "    sampled_theta = [np.random.beta(alpha[i], beta[i]) for i in range(3)]\n",
        "\n",
        "    chosen_bandit = np.argmax(sampled_theta)\n",
        "\n",
        "    # simulate the reward for the chosen bandit\n",
        "    reward = np.random.binomial(1, theta_true[chosen_bandit])\n",
        "\n",
        "    alpha[chosen_bandit] += reward\n",
        "    beta[chosen_bandit] += 1 - reward\n",
        "\n",
        "    alpha_dirichlet[chosen_bandit] += reward\n",
        "\n",
        "    # update tracking variables\n",
        "    rewards.append(reward)\n",
        "    cumulative_rewards.append(sum(rewards))\n",
        "    for i in range(3):\n",
        "        posterior_means[i].append(alpha[i] / (alpha[i] + beta[i]))\n",
        "    posterior_means_dirichlet.append(alpha_dirichlet / np.sum(alpha_dirichlet))\n",
        "\n",
        "# posterior means (Beta-Binomial) over time\n",
        "plt.figure(figsize=(12, 6))\n",
        "for i in range(3):\n",
        "    plt.plot(posterior_means[i], label=f\"Bandit {i + 1}\")\n",
        "    plt.axhline(theta_true[i], linestyle=\"--\", label=f\"True {i + 1}\")\n",
        "\n",
        "plt.title(\"Posterior Means for Each Bandit Over Time (Beta-Binomial)\")\n",
        "plt.xlabel(\"Iterations\")\n",
        "plt.ylabel(\"Posterior Mean\")\n",
        "plt.legend()\n",
        "plt.grid()\n",
        "plt.show()\n",
        "\n",
        "# posterior means (Dirichlet-Multinomial) over time\n",
        "plt.figure(figsize=(12, 6))\n",
        "posterior_means_dirichlet = np.array(posterior_means_dirichlet)\n",
        "for i in range(3):\n",
        "    plt.plot(posterior_means_dirichlet[:, i], label=f\"Bandit {i + 1}\")\n",
        "\n",
        "plt.title(\"Posterior Means for Each Bandit Over Time (Dirichlet-Multinomial)\")\n",
        "plt.xlabel(\"Iterations\")\n",
        "plt.ylabel(\"Posterior Mean\")\n",
        "plt.legend()\n",
        "plt.grid()\n",
        "plt.show()\n",
        "\n",
        "# cumulative rewards over time\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.plot(cumulative_rewards, label=\"Cumulative Rewards\")\n",
        "plt.title(\"Cumulative Rewards Over Time\")\n",
        "plt.xlabel(\"Iterations\")\n",
        "plt.ylabel(\"Total Reward\")\n",
        "plt.legend()\n",
        "plt.grid()\n",
        "plt.show()"
      ],
