      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "from scipy.stats import norm, gamma\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def gibbs_sampler(n, mu_prior, sigma_prior):\n",
        "    x = norm(loc=mu_prior, scale=sigma_prior).rvs(size=n)\n",
        "    C, G = 2, 10000\n",
        "    theta, tau = np.zeros([C, G]), np.zeros([C, G])\n",
        "\n",
        "    theta[:, 0] = np.mean(x)\n",
        "\n",
        "    for c in range(C):\n",
        "        for g in range(1, G):\n",
        "            tau_shape = n / 2 + 1\n",
        "            tau_scale = 1 / (0.5 * ((x - theta[c, g-1])**2).sum())\n",
        "            tau[c, g] = gamma.rvs(a=tau_shape, scale=tau_scale)\n",
        "\n",
        "            # sample theta from normal distribution\n",
        "            theta_mean = np.mean(x)\n",
        "            theta_sd = np.sqrt(1 / (tau[c, g] * n))  # adjusted scale for normal\n",
        "            theta[c, g] = norm.rvs(loc=theta_mean, scale=theta_sd)\n",
        "\n",
        "    return theta, tau\n",
        "\n",
        "# run the gibbs sampler\n",
        "n, mu_prior, sigma_prior = 100, 0, 2  # Example inputs\n",
        "theta, tau = gibbs_sampler(n, mu_prior, sigma_prior)\n",
        "\n",
        "fig, ax = plt.subplots(1, 4, figsize=(14, 3))\n",
        "ax[0].set_title(\"$\\\\theta$ Burn-In\")\n",
        "ax[1].set_title(\"$\\\\tau$ Burn-In\")\n",
        "ax[2].set_title(\"$\\\\theta$ Converged Markov Chain\\nStationary Distribution Samples\")\n",
        "ax[3].set_title(\"$\\\\tau$ Converged Markov Chain\\nStationary Distribution Samples\")\n",
        "\n",
        "burn, demo = 20, 120\n",
        "for c in range(2):\n",
        "    ax[0].plot(theta[c, :burn], label=\"$\\\\theta$ Chain \" + str(c))\n",
        "    ax[1].plot(tau[c, :burn], label=\"$\\\\tau$ Chain \" + str(c))\n",
        "    ax[2].plot(np.arange(burn, demo, dtype=int), theta[c, burn:demo], label=\"$\\\\theta$ Chain \" + str(c))\n",
        "    ax[3].plot(np.arange(burn, demo, dtype=int), tau[c, burn:demo], label=\"$\\\\tau$ Chain \" + str(c))\n",
        "\n",
        "for a in ax:\n",
        "    a.legend()\n",
        "\n",
        "plt.show()\n"
      ],
