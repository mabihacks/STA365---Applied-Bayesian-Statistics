    {
      "cell_type": "code",
      "source": [
        "\n",
        "# hyperparameters for the Gamma prior\n",
        "alpha_prior = 2.0\n",
        "lambda_prior = 2.0\n",
        "\n",
        "# parameters\n",
        "n = 30\n",
        "x = norm.rvs(loc=0, scale=1, size=n)\n",
        "\n",
        "# gibbs sampling parameters\n",
        "C, G = 2, 10000\n",
        "\n",
        "theta_samples = np.zeros((C, G))\n",
        "tau_samples = np.zeros((C, G))\n",
        "\n",
        "theta_samples[:, 0] = 1000\n",
        "\n",
        "for c in range(C):\n",
        "    for g in range(1, G):  s\n",
        "        # update tau (Gamma posterior)\n",
        "        tau_shape = alpha_prior + n / 2\n",
        "        tau_rate = lambda_prior + 0.5 * np.sum((x - theta_samples[c, g - 1]) ** 2)\n",
        "        tau_samples[c, g] = gamma.rvs(a=tau_shape, scale=1 / tau_rate)\n",
        "\n",
        "        # update theta (Normal posterior)\n",
        "        theta_mean = np.mean(x)\n",
        "        theta_sd = np.sqrt(1 / (n * tau_samples[c, g]))\n",
        "        theta_samples[c, g] = norm.rvs(loc=theta_mean, scale=theta_sd)\n",
        "\n",
        "# plot results\n",
        "burn_in = 20\n",
        "sample_range = 120\n",
        "fig, ax = plt.subplots(1, 4, figsize=(14, 3))\n",
        "\n",
        "# burn-in phase\n",
        "ax[0].plot(theta_samples[0, :burn_in], label=r\"$\\theta$ Chain 0\")\n",
        "ax[0].plot(theta_samples[1, :burn_in], label=r\"$\\theta$ Chain 1\")\n",
        "ax[0].set_title(r\"$\\theta$ Burn-In\")\n",
        "ax[0].legend()\n",
        "\n",
        "ax[1].plot(tau_samples[0, :burn_in], label=r\"$\\tau$ Chain 0\")\n",
        "ax[1].plot(tau_samples[1, :burn_in], label=r\"$\\tau$ Chain 1\")\n",
        "ax[1].set_title(r\"$\\tau$ Burn-In\")\n",
        "ax[1].legend()\n",
        "\n",
        "# converged markov chains\n",
        "ax[2].plot(range(burn_in, sample_range), theta_samples[0, burn_in:sample_range], label=r\"$\\theta$ Chain 0\")\n",
        "ax[2].plot(range(burn_in, sample_range), theta_samples[1, burn_in:sample_range], label=r\"$\\theta$ Chain 1\")\n",
        "ax[2].set_title(r\"Converged $\\theta$ Samples\")\n",
        "ax[2].legend()\n",
        "\n",
        "ax[3].plot(range(burn_in, sample_range), tau_samples[0, burn_in:sample_range], label=r\"$\\tau$ Chain 0\")\n",
        "ax[3].plot(range(burn_in, sample_range), tau_samples[1, burn_in:sample_range], label=r\"$\\tau$ Chain 1\")\n",
        "ax[3].set_title(r\"Converged $\\tau$ Samples\")\n",
        "ax[3].legend()\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
      "metadata": {
